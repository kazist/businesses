    "query": {
        "from": {
            "table": "Businesses\\Businesses\\Products\\Images\\Code\\Tables\\Images",
            "alias": "bbpi",
            "name": "#__businesses_businesses_products_images"
        },
        "select": [
            "bbpi.id",
            "bbpi.product_id",
            "bbp.title AS product_id_title",
            "bbpi.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "bbpi.created_by",
            "uu.name AS created_by_name",
            "bbpi.date_created",
            "bbpi.modified_by",
            "uu_4815.name AS modified_by_name",
            "bbpi.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Businesses\\Businesses\\Products\\Code\\Tables\\Products",
                "alias": "bbp",
                "name": "#__businesses_businesses_products",
                "on": "bbpi.product_id = bbp.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "bbpi.image = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "bbpi.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4815",
                "name": "#__users_users",
                "on": "bbpi.modified_by = uu_4815.id"
            }
        ]
    }
