    "query": {
        "from": {
            "table": "Businesses\\Businesses\\Code\\Tables\\Businesses",
            "alias": "bb",
            "name": "#__businesses_businesses"
        },
        "select": [
            "bb.id",
            "bb.category_id",
            "bc.title AS category_id_title",
            "bb.name",
            "bb.profile",
            "bb.physical_location",
            "bb.address",
            "bb.phone",
            "bb.email",
            "bb.website",
            "bb.country_id",
            "sc.name AS country_id_name",
            "bb.location_id",
            "bb.city",
            "bb.latitude",
            "bb.longitude",
            "bb.logo",
            "mm.title AS logo_title",
            "mm.file AS logo_file",
            "bb.package_id",
            "bp.title AS package_id_title",
            "bb.package_price_id",
            "bpp.price AS package_price_id_price",
            "bpp.package_id AS package_price_id_package_id",
            "bb.payment_amount",
            "bb.payment_stage",
            "bb.payment_status",
            "bb.payment_date",
            "bb.payment_expiry",
            "bb.hit",
            "bb.published",
            "bb.featured",
            "bb.created_by",
            "bb.date_created",
            "bb.modified_by",
            "bb.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Businesses\\Categories\\Code\\Tables\\Categories",
                "alias": "bc",
                "name": "#__businesses_categories",
                "on": "bb.category_id = bc.id"
            },
            {
                "table": "Setup\\Countries\\Code\\Tables\\Countries",
                "alias": "sc",
                "name": "#__setup_countries",
                "on": "bb.country_id = sc.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "bb.logo = mm.id"
            },
            {
                "table": "Businesses\\Packages\\Code\\Tables\\Packages",
                "alias": "bp",
                "name": "#__businesses_packages",
                "on": "bb.package_id = bp.id"
            },
            {
                "table": "Businesses\\Packages\\Prices\\Code\\Tables\\Prices",
                "alias": "bpp",
                "name": "#__businesses_packages_prices",
                "on": "bb.package_price_id = bpp.id"
            }
        ]
    }
}